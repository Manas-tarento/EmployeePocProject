{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "age": {
      "type": "integer"
    },
    "designation": {
      "type": "string"
    },
    "dob": {
      "type": "string",
      "format": "date"
    }
  },
  "required": ["name", "age", "designation", "dob"],
  "additionalProperties": true
}